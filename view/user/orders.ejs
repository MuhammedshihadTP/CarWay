<div class="shopping-cart section">
  <div class="container">
    <div class="row">
      <div class="col-12">
        <!-- Shopping Summery -->
        <table class="table shopping-summery">
          <h1>My Orders</h1>
          <thead>
            <tr class="main-hading">
              <th></th>
              <th>Vhicle Name</th>
              <th>Email</th>
              <th>Address</th>
              <!-- <th class="text-center"></th> -->
              <!-- <th class="text-center">QUANTITY</th> -->
              <th class="text-center">PRICE</th>
           
            </tr>
          </thead>
             <tbody>
              <% for (let i = 0; i < order.length; i++) { %>
                <% const cartItems = order[i].cart.items %>
                <% for (let j = 0; j < cartItems.length; j++) { %>
                  <tr>
                    <td class="image" data-title="No"><img src="../images/<%=cartItems[j].productId.image %>" alt="#"></td>
                    <td><%= cartItems[j].productId.name %></td>
                    <td><%= order[i].email %></td>
                    <td><%= order[i].address %></td>
                    <td>â‚¹<%= cartItems[j].price %></td>
                  </tr>
                <% } %>
              <% } %>
            </tbody>
        
        </table>
      </div>
    </div>
  </div>
</div>